lalt - return 	: /Applications/kitty.app/Contents/MacOS/kitty --single-instance -d ~
lalt - F	: open -na /Applications/Firefox.app
lalt - D	: open -na /Applications/kitty.app

# Functions
lctrl - V	: skhd -k "cmd - V"
lctrl - C	: skhd -k "cmd - C"
lctrl - T [
 "Firefox"	: skhd -k "cmd - T"
 *		: ~
]
